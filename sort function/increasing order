#include<bits/stdc++.h>
using namespace std;
int main()
{
  int arr[5]={5,4,2,1,7};
  //sort function
  sort(a,a+5)  //a->first element address,a+5->last element address
  for(int i=0;i<5;i++)
  {
    cout<<arr[i]<<" ";
  }
  cout<<"\n";
  vector<int>ve={5,4,2,1,7};
  sort(ve.begin(),ve.end());
  for(auto it:ve)
  {
    cout<<it<<" ";
  }
  return 0;
 }
